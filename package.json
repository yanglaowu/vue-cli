{
  "name": "vue-cli",
  "description": "vue-cli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build:dev": "cross-env NODE_ENV=production vue-cli-service build --mode dev",
    "build:dll": "webpack --progress --config ./webpack.dll.js",
    "build:prod": "cross-env NODE_ENV=production vue-cli-service build --mode prod",
    "release:dev": "cross-env NODE_ENV=development node ./deploy",
    "serve:dev": "cross-env NODE_ENV=development vue-cli-service serve --mode dev",
    "serve:prod": "cross-env NODE_ENV=development vue-cli-service serve --mode prod"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "crypto-js": "^4.0.0",
    "element-ui": "^2.13.2",
    "lodash": "^4.17.19",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-i18n": "^8.21.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "hard-source-webpack-plugin": "^0.13.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "scp2": "^0.5.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "sideEffects": [
    "*.vue"
  ]
}
